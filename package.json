{
  "name": "addin-next-test",
  "version": "0.1.0",
  "private": true,
  "config": {
    "app_to_debug": "word",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "dev": "ts-node server.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"src/{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"src/{app,lib,components}**/*.{ts,tsx,mdx}\" --cache",
    "odev": "office-addin-debugging start manifest-local.xml",
    "odev:desktop": "office-addin-debugging start manifest-local.xml desktop",
    "odev:web": "office-addin-debugging start manifest-local.xml web",
    "ostop": "office-addin-debugging stop manifest-local.xml",
    "ovalidate": "office-addin-manifest validate manifest-local.xml"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@vercel/kv": "^0.2.1",
    "ai": "^2.1.6",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "nanoid": "^4.0.2",
    "next": "13.4.12",
    "next-auth": "4.22.3",
    "next-themes": "^0.2.1",
    "openai-edge": "^0.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^8.0.7",
    "react-textarea-autosize": "^8.4.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.4.5",
    "@types/office-js": "^1.0.338",
    "@types/office-runtime": "^1.0.33",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "10.4.14",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "office-addin-debugging": "^5.0.11",
    "office-addin-dev-certs": "^1.11.5",
    "office-addin-manifest": "^1.12.6",
    "postcss": "8.4.27",
    "prettier": "^2.7.1",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.5",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "browser": {
    "crypto": false,
    "querystring": false,
    "http": false,
    "https": false
  }
}
